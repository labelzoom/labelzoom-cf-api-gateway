                    "https://beta.labelzoom.net",
                    "https://gatsby.labelzoom.net",
                ],
                "LZ_LOG_SAMPLE_RATE": 0.001,
                "LZ_ERROR_SAMPLE_RATE": 1,
                "LZ_PROD_API_BASE_URL": "https://prod-api-eus2-backend.labelzoom.net",
